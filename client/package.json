{
  "name": "uncover",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:3001",
  "dependencies": {
    "node-sass": "^4.13.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-is": "^17.0.2",
    "react-map-gl": "^6.1.16",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.1",
    "sass-loader": "^8.0.2",
    "styled-components": "^5.3.0",
    "superagent": "^5.2.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "install:clean": "rm -rf node_modules/ && rm -rf yarn.lock && yarn && yarn start",
    "compile-sass": "node-sass src/assets/scss/app.scss src/assets/css/app.css",
    "minify-sass": "node-sass src/assets/scss/app.scss src/assets/css/app.min.css --output-style compressed",
    "map-sass": "node-sass src/assets/scss/app.scss src/assets/css/app.css --source-map true"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
